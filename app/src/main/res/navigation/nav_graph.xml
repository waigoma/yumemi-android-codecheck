<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/repoSearchFragment">

    <fragment
        android:id="@+id/repoSearchFragment"
        android:name="jp.co.yumemi.android.codecheck.RepoSearchFragment"
        android:label="@string/app_name"
        tools:layout="@layout/repo_search_fragment">
        <action
            android:id="@+id/action_repoSearchFragment_to_repoViewFragment"
            app:destination="@id/twoFragment" />
    </fragment>

    <fragment
        android:id="@+id/twoFragment"
        android:name="jp.co.yumemi.android.codecheck.TwoFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_two">
        <argument
            android:name="item"
            app:argType="jp.co.yumemi.android.codecheck.Item" />
    </fragment>

</navigation>
